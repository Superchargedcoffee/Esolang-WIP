1					// Starting point
let x 1 end				// x will keep track of fizzes and buzzes
: 3 % 0 = if 0 "Fizz" prw let x end end	// If n % 3 == 0, print Fizz and set x = 0
: 5 % 0 = if 0 "Buzz" prw let x end end // If n % 5 == 0, print Buzz and set x = 0
x if : prn end 10 prc			// If x = 1, print n. Print a new line.
: 100 < if 1 + 1 0 jump end		// If n < 100, increment and jump to top.
